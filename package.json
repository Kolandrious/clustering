{
  "name": "diplom",
  "version": "1.0.0",
  "description": "",
  "main": "./index.ts",
  "author": "Vlad Klimov",
  "license": "ISC",
  "keywords": [
    "clustering"
  ],
  "type": "module",
  "scripts": {
    "ts": "node --experimental-specifier-resolution=node --loader ts-node/esm.mjs",
    "start": "nodemon",
    "debug": "nodemon --exec yarn ts --inspect=5858 ./index",
    "start:mongo": "docker start diplom_mongo",
    "build": "tsc",
    "docker": "docker-compose up",
    "mongo": "docker exec -it diplom_mongo mongo",
    "feed": "npm run ts ./src/scripts/feed",
    "clusterize": "npm run ts ./src/scripts/clusterize",
    "benchmark": "npm run ts ./src/scripts/benchmark"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv-extended": "^2.8.0",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "geojson-rbush": "^3.1.2",
    "geokdbush": "^1.1.0",
    "kdbush": "^3.0.0",
    "mongoose": "^5.9.13",
    "rbush": "^3.0.1",
    "rbush-knn": "^3.0.1",
    "uuid": "^8.0.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/geokdbush": "^1.1.2",
    "@types/kdbush": "^3.0.0",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^14.0.6",
    "@types/rbush": "^3.0.0",
    "@types/uuid": "^8.0.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
